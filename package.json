  "license": "MIT",
  "author": "Damian Tarnawski <gthetarnav@gmail.com>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thetarnav/rallax.git"
  },
  "homepage": "https://github.com/thetarnav/rallax#readme",
  "bugs": {
    "url": "https://github.com/thetarnav/rallax/issues"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
