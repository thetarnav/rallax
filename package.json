    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite serve dev",
    "test": "echo \"test not setup\"",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "taze -w && pnpm i",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@solid-primitives/resize-observer": "^2.0.2",
    "@solid-primitives/scroll": "^2.0.2",
    "@solid-primitives/utils": "^2.2.1"
  },
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "devDependencies": {
    "@unocss/reset": "^0.44.7",
    "prettier": "2.7.1",
    "rollup": "^2.77.2",
    "rollup-preset-solid": "^1.4.0",
    "solid-js": "^1.4.8",
    "taze": "^0.7.6",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "unocss": "^0.44.7",
    "vite": "^3.0.4",
    "vite-plugin-solid": "^2.3.0"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@7.5.0"
